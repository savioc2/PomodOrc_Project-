{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "sourceMap": true,
    "noImplicitAny": true,
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts", "src/models/user.js", "src/models/taskList.js"],
  "exclude": ["node_modules"],
}